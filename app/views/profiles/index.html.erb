<h1>Profiles</h1>

<table class="table table-striped">
  <tr>
    <th>Name</th>
    <th>Bio</th>
    <th>Birth Day</th>
    <th>BMI</th>
    <th>YMCA</th>
  </tr>

<% @profiles.each do |profile| %>
  <tr>
    <td><%= link_to profile.name, profile_path(profile) %></td>
    <td><%= profile.bio %></td>
    <td><%= profile.born_on %></td>
    <td><%= profile.bmi %></td>
    <td><%= profile.ymca_name %></td>
  </tr>
<% end %>
</table>

<br />

